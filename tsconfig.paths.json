{
    "compilerOptions": {
        "paths": {
            "@views/*": ["./src/views/*"],
            "@components/*": ["./src/components/*"],

            "@layouts/*": ["./src/layouts/*"],
            "@services/*": ["./src/services/*"],
            "@types/*": ["./src/types/*"],

            "@store/*": ["./src/store/*"],
            "@redux-slices/*": ["./src/redux-slices/*"],
            "@config/*": ["./src/config/*"],
            "@assets/*": ["./src/assets/*"],
            "@common/*": ["./src/assets/stylesheets/_index.scss"],
            "@locales/*": ["./src/locales/*"],
            "@helpers/*": ["./src/helpers/*"],
            "@enums/*": ["./src/helpers/enums/*"],
            "@interfaces/*": ["./src/helpers/interfaces/*"],
            "@pages/*": ["./src/pages/*"],
            "@containers/*": ["./src/containers/*"]
        }
    }
}
